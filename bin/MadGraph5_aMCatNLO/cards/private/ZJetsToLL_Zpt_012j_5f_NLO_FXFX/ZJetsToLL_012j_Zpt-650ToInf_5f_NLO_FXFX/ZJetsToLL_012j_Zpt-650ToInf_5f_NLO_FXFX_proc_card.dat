import model loop_sm-ckm_no_b_mass
#switch to diagonal ckm matrix if needed for speed
#import model loop_sm-no_b_mass

define p = p b b~
define j = j b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-

generate p p > Z > ell+ ell- [QCD] @0
add process p p > Z > ell+ ell- j [QCD] @1
add process p p > Z > ell+ ell- j j [QCD] @2

output ZJetsToLL_012j_Zpt-650ToInf_5f_NLO_FXFX -nojpeg

